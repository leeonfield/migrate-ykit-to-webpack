    "core-decorators": "^0.17.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.5.3",
    "eslint": "3.19.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.1.0",
