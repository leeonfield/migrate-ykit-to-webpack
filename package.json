{
  "name": "migrate-ykit-to-webpack",
  "version": "1.0.0",
  "description": "Yapi 构建升级 webpack 4",
  "main": "babel.config.js",
  "scripts": {
    "start": "node server/app.js",
    "install-server": "node server/install.js",
    "dev": "npm run dev-server & npm run dev-client",
    "dev-client": "webpack-dev-server --config ./config/webpack.dev.conf.js",
    "dev-server": "cross-env NODE_ENV=development nodemon server/app.js dev -L",
    "build-client": "webpack --config ./config/webpack.prod.conf.js",
    "test": "ava",
    "npm-publish": "node ./npm-publish.js",
    "fix": "eslint --fix --ext .js,.jsx client/",
    "dep-analyze": "webpack --color --config webpack.dev --profile --json > ./dist/DependenceNetwork.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leeonfield/migrate-ykit-to-webpack.git"
  },
