    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.6",
    "assets-webpack-plugin": "^3.5.1",
    "ava": "^0.22.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
