    "terser-webpack-plugin": "^3.0.1",
    "thread-loader": "^2.1.3",
    "url-loader": "^4.1.0",
    "validate-commit-msg": "^2.12.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
